<apex:page tabStyle="Employeee__c" standardController="Employeee__c">
<apex:form >

        <apex:pageBlock title=" Employee Details">
        <apex:pageBlockButtons >
        <apex:commandButton value="Save" action="{!save}"/>
        <apex:commandButton value="Cancel" action="{!cancel}"/>
        </apex:pageBlockButtons>
      
        
        <apex:pageBlockSection title="Employee Information" collapsible="false">
        <apex:pageBlockSectionItem >
        <apex:outputLabel value="Employee Name"/>
        <apex:inputField value="{!Employeee__c.Employename__c}"/> 
        </apex:pageBlockSectionItem><br/>
        
                              <apex:pageBlockSectionItem >
                              <apex:outputLabel value="Depament Number"/>
                              <apex:inputField value="{!Employeee__c.Depament_Number__c}"/>
                              </apex:pageBlockSectionItem><br/>
                              
                   <apex:pageBlockSectionItem >
                   <apex:outputLabel value="Job Title"/>
                   <apex:inputField value="{!Employeee__c.JobTitle__c}"/>
                   </apex:pageBlockSectionItem><br/>
                   
                                           <apex:pageBlockSectionItem >
                                           <apex:outputLabel value="Salary"/>
                                           <apex:inputField value="{!Employeee__c.Salary__c}"/>
                                           </apex:pageBlockSectionItem><br/>
                                           
                                           
                   <apex:pageBlockSectionItem >
                   <apex:outputLabel value="Country"/>
                   <apex:inputField value="{!Employeee__c.Country__c}"/>
                   </apex:pageBlockSectionItem>
        
        
</apex:pageBlockSection>
</apex:pageBlock>  
</apex:form>
</apex:page>