<apex:page standardController="Employeee__c">
<apex:form >
       <apex:pageBlock title="Employe Detials">
       <apex:pageBlockButtons >
               <apex:commandButton value="SAVE" action="{!save}"/>
               <apex:commandButton value="CANCEL" action="{!cancel}"/>
               <apex:commandButton value="EDIT" action="{!edit}"/>
               <apex:commandButton value="SAVE & NEW" action="{!quickSave}"/>
               <apex:commandButton value="DELETE" action="{!delete}"/>
       </apex:pageBlockButtons>
       
       <apex:pageBlockSection >
           <apex:pageblockSectionItem >
               <apex:outputLabel >Employe Name :</apex:outputLabel>
               <apex:outputField value="{!Employeee__c.Employename__c}"/>
           </apex:pageblockSectionItem>
     
       
       <apex:pageBlockSectionItem >
       <apex:outputLabel > Country</apex:outputLabel>
       <apex:outputField value="{!Employeee__c.Country__c}"/>
       </apex:pageBlockSectionItem>
       
       <apex:pageBlockSectionItem >
       <apex:outputLabel > Depament Number</apex:outputLabel>
       <apex:outputField value="{!Employeee__c.Depament_Number__c}"/>
       </apex:pageBlockSectionItem>
       
        <apex:pageBlockSectionItem >
       <apex:outputLabel > Job Title</apex:outputLabel>
       <apex:outputField value="{!Employeee__c.JobTitle__c}"/>
       </apex:pageBlockSectionItem>
       
       <apex:pageBlockSectionItem >
       <apex:outputLabel > Salary</apex:outputLabel>
       <apex:outputField value="{!Employeee__c.Salary__c}"/>
       </apex:pageBlockSectionItem>
         </apex:pageBlockSection>
</apex:pageBlock>
</apex:form>
</apex:page>